<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.amtzhmt.launcher.channelplay.ChannelplayActivity">

    <com.amtzhmt.launcher.util.utils.customizeview.MyVideoView
        android:id="@+id/channelplay"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <TextView
        android:id="@+id/channelnumber"
        android:textColor="@color/whithe"
        android:textSize="70sp"
        android:textStyle="bold"
        android:text="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="40dp"
        android:layout_gravity="right|top" />


</FrameLayout>
